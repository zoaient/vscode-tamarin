{
  "name": "tamarin-prover",
  "version": "2.1.0",
  "engines": {
    "vscode": "^1.90.0"
  },
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/tamarin-prover/vscode-tamarin.git"
  },
  "publisher": "tamarin-prover",
  "categories": [
    "Programming Languages",
    "Snippets",
    "Extension Packs"
  ],
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "tamarin",
        "aliases": [
          "tamarin"
        ],
        "extensions": [
          ".spthy",
          ".splib"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tamarin",
        "scopeName": "source.tamarin",
        "path": "./syntaxes/tamarin.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "tamarin",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [
      {
        "command": "extension.tabKeyBinding",
        "title": "Navigate to next symbol",
        "when": "editorLangId == tamarin"
      },
      {
        "command": "myExtension.navigateToNextSymbol",
        "title": "Navigate to Next Symbol",
        "when": "editorLangId == tamarin"
      }
    ],
    "keybindings": [
      {
        "key": "ctrl+alt+n",
        "command": "extension.tabKeyBinding",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "title": "Tamarin",
      "properties": {}
    }
  },
  "main": "./client/out/extension.js",
  "types": "bindings/node",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "clean": "rimraf client/out server/out client/tsconfig.tsbuildinfo server/tsconfig.tsbuildinfo client/node_modules server/node_modules node_modules",
    "compile": "tsc -b tsconfig.json && cp -r server/grammar server/out/",
    "lint": "eslint client/src server/src --fix --ext ts",
    "watch": "tsc -b tsconfig.json -w",
    "pretest": "npm run compile && npm run lint",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd ..",
    "test": "jest",
    "prebuildify": "prebuildify --napi --strip"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.0.0",
    "jest": "^30.0.4",
    "ovsx": "^0.8.1",
    "prebuildify": "^6.0.0",
    "tree-sitter-cli": "^0.22.6",
    "tree-sitter-javascript": "^0.21.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.4.5",
    "vsce": "^1.97.0",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.0",
    "tree-sitter": "^0.21.0",
    "tree-sitter-highlight": "^1.0.1",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "vscode-uri": "^3.1.0",
    "web-tree-sitter": "^0.22.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": false
    }
  },
  "license": "MIT",
  "tree-sitter": [
    {
      "scope": "source.spthy",
      "injection-regex": "^spthy$"
    }
  ]
}
